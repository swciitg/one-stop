{
    "tags": ["users"],
    "summary": "Get personal notifications",
    "operationId": "getUserPersonalNotifs",
    "security": [
      {
        "bearerAuth": []
      }
    ],
    "responses": {
      "200": {
        "description": "Notifications retrieved",
        "content": {
          "application/json": {
            "schema": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "message": {
                    "type": "string",
                    "example": "New event happening tomorrow!"
                  },
                  "date": {
                    "type": "string",
                    "format": "date-time",
                    "example": "2024-10-01T12:00:00Z"
                  }
                }
              }
            }
          }
        }
      },
      "401": {
        "description": "Unauthorized"
      }
    }
  }
  